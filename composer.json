{
    "name": "chinhle-pa/attestation",
    "description": "A description for attestation.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Chinh Le",
            "email": "chinhle@pacificcross.com.vn"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0",
        "sop/asn1": "^4.1",
